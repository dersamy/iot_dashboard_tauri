  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "http:default",
    "http:allow-fetch",
    "http:default",
    {
      "identifier": "http:default",
      "allow": [ { "url": "https://echo.free.beeceptor.com"}],
      "deny": [{ "url": "https://private.tauri.app" }]
    }
  ] 
}
